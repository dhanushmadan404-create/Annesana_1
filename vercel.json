{
  "version": 2,
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11",
        "buildCommand": "pip install -r requirements.txt"
      }
    }
  ],
  "routes": [
    { "src": "/uploads/(.*)", "dest": "backend/uploads/$1" },
    { "src": "/(.*)", "dest": "backend/main.py" }
  ]
}
