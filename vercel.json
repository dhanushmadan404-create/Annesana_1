{
  "version": 2,
  "rewrites": [
    {
      "source": "/users(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/food(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/vendors(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/docs",
      "destination": "/api/index.py"
    },
    {
      "source": "/openapi.json",
      "destination": "/api/index.py"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/frontend/assets/$1"
    },
    {
      "source": "/styles/(.*)",
      "destination": "/frontend/styles/$1"
    },
    {
      "source": "/scripts/(.*)",
      "destination": "/frontend/scripts/$1"
    },
    {
      "source": "/pages/(.*)",
      "destination": "/frontend/pages/$1"
    },
    {
      "source": "/",
      "destination": "/frontend/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ]
}