{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/users/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/food/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/vendors/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/",
      "destination": "/frontend/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ]
}