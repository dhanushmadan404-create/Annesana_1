{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/users/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/food/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/vendors/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/docs",
      "destination": "/api/index.py"
    },
    {
      "source": "/openapi.json",
      "destination": "/api/index.py"
    },
    {
      "source": "/favicon.ico",
      "destination": "/frontend/assets/icons8-a-64.png"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ]
}