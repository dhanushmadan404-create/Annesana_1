{
  "version": 2,
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/users/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/users",
      "destination": "/api/index.py"
    },
    {
      "source": "/foods/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/vendors/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/docs",
      "destination": "/api/index.py"
    },
    {
      "source": "/openapi.json",
      "destination": "/api/index.py"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}