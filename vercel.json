{
  "version": 2,
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "backend/main.py"
    },
    {
      "source": "/uploads/(.*)",
      "destination": "backend/main.py"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}