{
  "rewrites": [
    {
      "source": "/users(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/auth(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/food(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/vendors(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/docs",
      "destination": "/api/index.py"
    },
    {
      "source": "/openapi.json",
      "destination": "/api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ]
}